{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src", "controllers", "models", "config", "middleware"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "default"
  }
}